<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Author     : Genesys Framework
    Description:
        Purpose of the document follows.
-->
<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="Notice" table="NOTICES">
    <id column="ID" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="TYPE" name="type" type="int"/>
    <property column="ACCOUNT_ID" name="accountId" type="int"/>
    <property column="KIND" name="kind" type="int"/>
    <property column="SUB_KIND" name="subKind" type="int"/>
    <property column="MSG_TITLE" name="msgTitle"/>
    <property column="MSG_CONTENT" name="msgContent"/>
    <property column="INVITEE_ID" name="inviteeId" type="int"/>
    <property column="ESTIMATE_ID" name="estimateId" type="int"/>
    <property column="ERROR_ID" name="errorId" type="int"/>
    <property column="STATUS" name="status" type="int"/>
    <property column="WRITE_TIME" name="writeTime" type="timestamp"/>
    
  </class>
</hibernate-mapping>