<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Author     : Genesys Framework
    Description:
        Purpose of the document follows.
-->
<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="VerifyCode" table="verify_codes">
    <id column="id" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="mobile" name="mobile"/>
    <property column="verify_code" name="verifyCode"/>
    <property column="write_time" name="writeTime" type="timestamp"/>
    
  </class>
</hibernate-mapping>