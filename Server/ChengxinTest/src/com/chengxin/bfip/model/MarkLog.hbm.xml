<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!--
    Author     : Genesys Framework
    Description:
        Purpose of the document follows.
-->
<hibernate-mapping package="com.chengxin.bfip.model">
  <class name="MarkLog" table="mark_logs">
    <id column="id" name="id" type="int">
      <generator class="identity" />
    </id>
    <property column="account_id" name="accountId" type="int"/>
    <property column="kind" name="kind" type="int"/>
    <property column="estimate_kind" name="estimateKind" type="int"/>
    <property column="estimate_method" name="estimateMethod" type="int"/>
    <property column="error_kind" name="errorKind" type="int"/>
    <property column="pmark" name="pmark" type="int"/>
    <property column="nmark" name="nmark" type="int"/>
    <property column="sender_id" name="senderId" type="int"/>
    <property column="write_time" name="writeTime" type="timestamp"/>
    
  </class>
</hibernate-mapping>