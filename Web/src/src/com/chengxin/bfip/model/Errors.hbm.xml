<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- 
	Author : Genesys Framework 
	Description: Purpose 
		of the document follows. 
-->
<hibernate-mapping package="com.chengxin.bfip.model">
	<class name="Errors" table="ERRORS">
		<id column="ID" name="id" type="int">
			<generator class="identity" />
		</id>
		<property column="ESTIMATE_ID" name="estimateId" type="int"/>
		<property column="OWNER_ID" name="ownerId" type="int"/>
		<property column="KIND" name="kind" type="int"/>
		<property column="REASON" name="reason" type="text"/>
		<property column="WHYIS" name="whyis" type="text"/>
		<property column="STATUS" name="status" type="int" />
		<property column="IMG_PATH1" name="imgPath1" />
		<property column="IMG_PATH2" name="imgPath2" />
		<property column="IMG_PATH3" name="imgPath3" />
		<property column="IMG_PATH4" name="imgPath4" />
		<property column="IMG_PATH5" name="imgPath5" />
		<property column="IMG_PATH6" name="imgPath6" />
		<property column="WRITE_TIME" name="writeTime" type="timestamp"/>

	</class>
</hibernate-mapping>